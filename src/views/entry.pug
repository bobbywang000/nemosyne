doctype html
html
  head
    meta(charset='utf-8')
    style
        include css/entry.css

    title #{date}
  body

  h1 #{date}
  a(href=prev) prev
  span
  a(href=next) next
  each entry in entries
    h2 Written #{entry.writeDate}
    h3 In ranges: #{JSON.stringify(entry.parentRanges)}
    | !{ entry.content }
